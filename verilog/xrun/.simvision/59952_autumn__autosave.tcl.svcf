#
# Design Browser windows
#
if {[catch {window new WatchList -name "Design Browser 1" -geometry 3834x1885+-1+27}] != ""} {
    window geometry "Design Browser 1" 3834x1885+-1+27
}
window target "Design Browser 1" on
browser using {Design Browser 1}
browser set -scope  {simulator::syndrome_tb.uut.gen_syndrome_slice[0].syndrome_slice_int.genblk2[12].mul_inst} 
browser set \
    -signalsort name
browser yview see  {simulator::syndrome_tb.uut.gen_syndrome_slice[0].syndrome_slice_int.genblk2[12].mul_inst} 
browser timecontrol set -lock 0

#
# Waveform windows
#
if {[catch {window new WaveWindow -name "Waveform 1" -geometry 3834x1885+-1+27}] != ""} {
    window geometry "Waveform 1" 3834x1885+-1+27
}
window target "Waveform 1" on
waveform using {Waveform 1}
waveform sidebar visibility partial
waveform set \
    -primarycursor TimeA \
    -signalnames name \
    -signalwidth 175 \
    -units ps \
    -valuewidth 75
waveform baseline set -time 0

